{
  "name": "billboard",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "lsof -ti:3000 | xargs kill -9 2>/dev/null || true && next dev",
    "dev:safe": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "playwright": "playwright test",
    "playwright:setup": "tsx scripts/setup-test-data.ts && playwright test",
    "turbo:build": "turbo build",
    "turbo:lint": "turbo lint",
    "turbo:lint:fix": "turbo lint:fix",
    "turbo:format": "turbo format",
    "turbo:format:check": "turbo format:check",
    "turbo:typecheck": "turbo typecheck",
    "turbo:test": "turbo test",
    "turbo:test:run": "turbo test:run",
    "turbo:playwright": "turbo playwright",
    "turbo:playwright:setup": "turbo playwright:setup",
    "turbo:dev:check": "turbo typecheck lint format:check",
    "playwright:ui": "playwright test --ui",
    "playwright:headed": "playwright test --headed",
    "playwright:debug": "playwright test --debug",
    "playwright:ci": "playwright test --reporter=html",
    "db:push": "echo 'Apply Supabase schema manually with the CLI or Studio'",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "db:seed:clean": "node scripts/seed.js --clean",
    "test:setup": "tsx scripts/setup-test-data.ts",
    "gen:schema": "echo 'Use supabase gen types --local > src/types/supabase.ts'",
    "quality": "pnpm typecheck && pnpm lint && pnpm format:check",
    "turbo:quality": "turbo quality",
    "prepare": "husky"
  },
  "dependencies": {
    "@alanszp/business-days-date-fns": "^16.0.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "2.45.6",
    "@tanstack/react-query": "^5.87.4",
    "@tanstack/react-query-devtools": "^5.87.4",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "date-fns": "^4.1.0",
    "date-holidays": "^3.25.2",
    "framer-motion": "11.2.10",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.2",
    "lucide-react": "0.441.0",
    "next": "15.5.2",
    "next-intl": "^4.3.7",
    "papaparse": "^5.5.3",
    "pdfmake": "^0.2.20",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.62.0",
    "react-i18next": "^15.7.3",
    "recharts": "2.12.7",
    "tailwind-merge": "2.5.2",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "^1.55.0",
    "@types/node": "20.14.9",
    "@types/papaparse": "^5.3.16",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "autoprefixer": "10.4.19",
    "dotenv": "^17.2.2",
    "eslint": "9.35.0",
    "eslint-config-next": "15.5.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "postcss": "8.4.39",
    "prettier": "3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "3.4.7",
    "tsx": "^4.20.5",
    "turbo": "^2.5.6",
    "typescript": "5.5.3",
    "vitest": "1.6.0"
  }
}
